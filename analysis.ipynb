    "def match_probability(constraints, words):\n",
    "   num_total_words = len(words)\n",
    "   for c in constraints:\n",
    "       words = filter(c.match, words)\n",
    "   words = list(words)\n",
    "   return len(words) / num_total_words\n",
    "\n",
    "\n"
   ]
  }
